
@model IEnumerable<Contoso_Unoversity_app_test.ViewModels.EnrollmentDateGroup>

@{
    ViewBag.Title = "Student Body Stats";
}

<table>
    <tr>
        <th>
            Enrollment Date
        </th>
        <th>
            Students
        </th>
    </tr>

    @foreach(var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EnrollmentDate)
            </td>
            <td>
                @item.StudentCount
            </td>
        </tr>

    }

</table>
